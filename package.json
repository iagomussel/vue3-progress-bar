{
  "name": "vue3-progress-bar",
  "description": "A progress bar component for Vue 3",
  "version": "2.0.0",
  "author": "Iago Mussel",
  "license": "MIT",
  "homepage": "https://github.com/iagomussel/vue3-progress-bar#readme",
  "private": false,
  "main": "./dist/ProgressBar.common.js",
  "browser": "dist/ProgressBar.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve ./test/main.js",
    "build": "vue-cli-service build --target lib --name ProgressBar src/index.js",
    "version": "npm run build && git add -A",
    "postversion": "git push -u --follow-tags origin master && npm publish"
  },
  "dependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "node-sass": "^8.0.0",
    "sass-loader": "^13.0.0",
    "vue-template-compiler": "^2.7.14"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iagomussel/vue3-progress-bar"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vuejs",
    "vue.js",
    "vue3",
    "component",
    "frontend",
    "progress",
    "progressbar",
    "progress-bar"
  ]
}
